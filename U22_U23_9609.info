FILE "U22_9609.bin" FF00
FILE 'U23_9609.bin' F800


remap    9F00-9F0B 4800 *to E000
remap    F8A2
remap    F84D-FC57 -9
remap    FC59-FC9E -C
remap    FCA0-FD01 1A
remap    FD1A      1A * TITLE
remap    FD24-FD2F 23
remap    FD30      38 * OK
remap    FD34      3A * BAD
remap    FD39-FDAD 3B

remap    FDAE-FFD8 6F
* Use the 9619 info file which is very similar
include  U7_9619.info

uncomment FFE0-FFFF
uncomment 0000-F800

unlabel  E000-F7FF
unlabel  FFDF-FFFF


label E3C0 ACAADDR0
label E3C8 ACAADDR1
label E3E0 TIMER

comment  F800 .
comment  F800     HARDWARE REQUIREMENTS:
comment  F800     THIS ROM AT $F800-$FFFF
comment  F800     RAM AT $E700-$E7FF
comment  F800     ACIA AT $E3C0
comment  F800     PTM AT $E3E0 (ENABLED TO NMI)
comment  F800 .
